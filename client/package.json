{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-xo-react": "^0.19.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.4.0",
    "xo": "^0.24.0"
  },
  "xo": {
    "extends": "xo-react",
    "space": true,
    "envs": [
      "browser"
    ],
    "overrides": [
      {
        "files": "src/*.js",
        "rules": {
          "unicorn/filename-case": [
            "error",
            {
              "case": "kebabCase"
            }
          ]
        }
      }
    ]
  }
}
