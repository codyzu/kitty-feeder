{
  "name": "kitty-feeder",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.3.1",
    "bootswatch": "^4.3.1",
    "firebase": "^5.9.3",
    "luxon": "^1.12.1",
    "query-string": "^6.5.0",
    "react": "^16.8.6",
    "react-day-picker": "^7.3.0",
    "react-dom": "^16.8.6",
    "react-icons": "^3.6.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.0",
    "reactstrap": "^8.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-ci": "yarn test",
    "pretest-ci": "yarn write-config",
    "eject": "react-scripts eject",
    "lint": "xo",
    "write-config": "echo $FIREBASE_CONFIG_JSON > src/firebase-config.json",
    "firebase": "firebase",
    "deploy": "firebase deploy",
    "deploy-ci": "yarn deploy --project=$FIREBASE_PROJECT"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-xo-react": "^0.19.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.4.0",
    "firebase-tools": "^6.5.3",
    "xo": "^0.24.0"
  },
  "xo": {
    "extends": "xo-react",
    "space": true,
    "envs": [
      "browser"
    ],
    "overrides": [
      {
        "files": "src/*.js",
        "rules": {
          "unicorn/filename-case": [
            "off"
          ]
        }
      },
      {
        "files": "src/*.test.js",
        "envs": [
          "jest",
          "browser"
        ]
      }
    ]
  }
}
